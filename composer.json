{
    "name": "phplrt/compiler",
    "type": "library",
    "description": "The phplrt compiler package.",
    "license": "MIT",
    "keywords": ["compiler", "builder", "grammar", "language"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/compiler"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laminas/laminas-code": "^4.0",
        "phplrt/lexer": "^3.1",
        "phplrt/visitor": "^3.1",
        "phplrt/position": "^3.1",
        "phplrt/parser": "^3.1",
        "phplrt/grammar": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Compiler\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Compiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev",
            "dev-main": "3.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
